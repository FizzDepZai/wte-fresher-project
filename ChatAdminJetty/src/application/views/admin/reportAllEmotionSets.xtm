                <span class="btn btn-danger"></span>
            </div>
        </div><!-- #sidebar-shortcuts -->

        <ul class="nav nav-list">

            <li>
                <a href="index.html">
                    <i class="icon-dashboard"></i>
                    <span>ChatRooms</span>

                </a>
            </li>


            <li>
                <a href="typography.html">
                    <i class="icon-text-width"></i>
                    <span>Emotions</span>

                </a>
            </li>
        </ul><!--/.nav-list-->
    </div><!--/#sidebar-->


    <div id="main-content" class="clearfix">

        <div id="breadcrumbs">
            <ul class="breadcrumb">
                <li><i class="icon-home"></i> <a href="#">Home</a><span class="divider"><i class="icon-angle-right"></i></span></li>
                <li class="active">reportSpecificEmotionSet>{{SetName}}</li>
            </ul><!--.breadcrumb-->

            <div id="nav-search">
                <form class="form-search">
                    <span class="input-icon">
                        <input autocomplete="off" id="nav-search-input" type="text" class="input-small search-query" placeholder="Search ..." />
                        <i id="nav-search-icon" class="icon-search"></i>
                    </span>
                </form>
            </div><!--#nav-search-->
        </div><!--#breadcrumbs-->
        <div class="row-fluid">
            <!-- PAGE CONTENT BEGINS HERE -->
            <form >
                <div class="widget-box">
                    <div class="widget-body">
                        <div class="widget-main">
                            <hr/>

                            <div class="control-group">
                                <div class="input-prepend">
                                    <div class="row-fluid">
                                        <label for="id-date-range-picker-1">Select Time Period</label>
                                    </div>
                                    <span class="add-on"><i class="icon-calendar"></i></span>
                                    <input class="span11" type="text" name="date-range-picker" id="id-date-range-picker-1" />
                                </div>
                                <div>
                                    <div class="row-fluid">
                                        <label for="id-date-range-picker-1">Select Type Period</label>
                                    </div>
                                    <select name = "TypeOfPeriod">
                                        <option>By Date</option>
                                        <option>By Week</option>
                                        <option>By Month</option>
                                        <option>By Year</option>
                                    </select>
                                </div>
                            </div>
                            <input type="submit" value="ViewSetChart">
                            <hr />
                        </div>
                    </div>
                </div>

            </form>

            <!-- PAGE CONTENT ENDS HERE -->
        </div><!--/row-->
        <!--Draw Chart-->
        <div id="chart_div" style="width: 1400px; height: 500px;"></div>
    </div><!--/#page-content-->

</div><!-- #main-content -->
